<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mdmd.entity.SysPropEntity" table="sys_prop" schema="happyfish">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="identity"/>
        </id>
        <property name="sysNum">
            <column name="sys_num" sql-type="int(11)"/>
        </property>
        <property name="sysName">
            <column name="sys_name" sql-type="varchar(25)" length="25"/>
        </property>
        <property name="sysValue">
            <column name="sys_value" sql-type="varchar(99)" length="99"/>
        </property>
    </class>
</hibernate-mapping>